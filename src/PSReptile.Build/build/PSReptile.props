<Project>
    <!-- The help file to generate -->
    <PropertyGroup>
        <BuildPSHelp Condition=" '$(BuildPSHelp)' == '' ">true</BuildPSHelp>
        <PublishPSHelp Condition=" '$(PublishPSHelp)' == '' ">true</PublishPSHelp>
    </PropertyGroup>

    <ItemGroup>
        <PSModuleHelp
            Include="@(BuiltProjectOutputGroupKeyOutput->'%(Identity)-Help.xml')"
            SourceModule="@(BuiltProjectOutputGroupKeyOutput)"
        />
    </ItemGroup>
</Project>
